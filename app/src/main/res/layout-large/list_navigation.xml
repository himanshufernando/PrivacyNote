<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <RelativeLayout
        android:layout_width="match_parent"
        android:id="@+id/relativeLayout7"
        android:layout_height="60dip"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="5dp">


        <RelativeLayout
            android:layout_height="match_parent"
            android:layout_alignParentTop="true"
            android:layout_alignParentLeft="true"
            android:id="@+id/relativeLayout9"
            android:layout_alignParentStart="true"
            android:layout_width="60dip"
            android:layout_marginLeft="10dp">

            <tkhub.project.PrivacyNote.ui.font.TextViewFontAwesome
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="New"
                android:id="@+id/imageViewnavigationIcon"
                android:textSize="40sp"
                android:textColor="@color/iconBlue"
                android:layout_centerVertical="true"
                android:layout_centerHorizontal="true" />
        </RelativeLayout>

        <tkhub.project.PrivacyNote.ui.font.FontSpalsh
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Medium Text"
            android:id="@+id/textViewNavigationName"
            android:textStyle="bold"
            android:textColor="#ffffff"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:layout_centerVertical="true"
            android:layout_toRightOf="@+id/relativeLayout9"
            android:layout_toEndOf="@+id/relativeLayout9"
            android:layout_marginLeft="10dp" />


    </RelativeLayout>

</RelativeLayout>
